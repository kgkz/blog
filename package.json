{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "dev:api": "aspida --watch",
    "build": "pathpida && aspida && next build",
    "api:build": "aspida",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "cz": "git-cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint . --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "axios": "^0.23.0",
    "framer-motion": "^4",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.30",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  }
}
