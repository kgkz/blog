{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "dev:api": "aspida --watch",
    "build": "pathpida && aspida && next build && next-sitemap",
    "api:build": "aspida",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "cz": "git-cz",
    "analyze": "ANALYZE=true next build"
  },
  "lint-staged": {
    "!(*path).{ts,tsx,js}": [
      "eslint . --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aspida/axios": "^1.10.3",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@types/react-dom": "18.0.6",
    "axios": "^1.1.3",
    "dayjs": "^1.11.5",
    "form-data": "^4.0.0",
    "highlight.js": "^11.6.0",
    "next": "^13.0.5",
    "next-sitemap": "^3.1.43",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.4",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "15.5.0",
    "tocbot": "^4.19.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.6",
    "@types/react": "18.0.25",
    "@types/react-syntax-highlighter": "15.5.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
